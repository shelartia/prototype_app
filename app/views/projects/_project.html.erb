  <% if project.valid? %>
    <div class="panel panel-primary">
      <div class="panel-heading">
        <h3 class="panel-title"><%=  project.name %></h3>
      </div>
      <div class="panel-body">
        <ul>
        <% project.tasks.each do |t|%>
          <li>  <%= t.name %> </li>
        <% end %>
        </ul>
      </div>
    </div>
  <% end %>